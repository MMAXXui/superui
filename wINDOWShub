local Tab4 = Window:MakeTab({
	Name = "Ball Modifications",
	Icon = "",
	PremiumOnly = false
})

Tab4:AddTextbox({
	Name = "Ball Size",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		local bal = game.Workspace.TPSSystem.TPS
        bal.Size = Vector3.new(Value, Value, Value)
	end	  
})


Tab4:AddToggle({
	Name = "Enable Ball Collision ",
	Default = false,
	Callback = function(arg)
		if arg then
            game.Workspace.TPSSystem.TPS.CanCollide = false
            else
            game.Workspace.TPSSystem.TPS.CanCollide = true
            end
	end    
})


Tab4:AddToggle({
	Name = "Disable Ball Textures ",
	Default = false,
	Callback = function(arg)
		if arg then
            game.Workspace.TPSSystem.TPS.Decal1.Texture = ""
            game.Workspace.TPSSystem.TPS.Decal2.Texture = ""
            game.Workspace.TPSSystem.TPS.Decal3.Texture = ""
            game.Workspace.TPSSystem.TPS.Decal4.Texture = ""
            game.Workspace.TPSSystem.TPS.Decal5.Texture = ""
            game.Workspace.TPSSystem.TPS.Decal6.Texture = ""
            else
            game.Workspace.TPSSystem.TPS.Decal1.Texture = "rbxassetid://1731401359"
            game.Workspace.TPSSystem.TPS.Decal2.Texture = "rbxassetid://1731401359"
            game.Workspace.TPSSystem.TPS.Decal3.Texture = "rbxassetid://1731401811"
            game.Workspace.TPSSystem.TPS.Decal4.Texture = "rbxassetid://1731401811"
            game.Workspace.TPSSystem.TPS.Decal5.Texture = "rbxassetid://1731401359"
            game.Workspace.TPSSystem.TPS.Decal6.Texture = "rbxassetid://1731401359"
            end
	end    
})


Tab4:AddToggle({
	Name = "Disable Ball Sounds ",
	Default = false,
	Callback = function(arg)
		if arg then
            game.Workspace.TPSSystem.TPS.Sound.SoundId = ""
            game.Workspace.TPSSystem.TPS.SoundFire.SoundId = ""

            else
            game.Workspace.TPSSystem.TPS.Sound.SoundId = "rbxassetid://2516069845"
            game.Workspace.TPSSystem.TPS.SoundFire.SoundId = "rbxassetid://3777051775"

            end
	end    
})

local part = game.Workspace.TPSSystem.TPS  
local pointLight1 = Instance.new("PointLight")

Tab4:AddToggle({
	Name = "Make The Ball Glowing  ",
	Default = false,
	Callback = function(arg)
		if arg then
pointLight1.Brightness = 2  
pointLight1.Range = 15 
pointLight1.Color = Color3.fromRGB(255, 255, 255)  
pointLight1.Shadows = false
pointLight1.Parent = part
pointLight1.Enabled = true
pointLight1.Name = "Point5"
            else
                pointLight1.Enabled = false
            end
	end    
})

Tab4:AddSlider({
	Name = "Ball Transparency",
	Min = 0,
	Max = 1,
	Default = 0,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.1,
	ValueName = "",
	Callback = function(Value)
		local Ball = game.Workspace.TPSSystem.TPS
        Ball.Transparency = Value
	end    
})

