local Tab1 = Window:MakeTab({
	Name = "Reacts",
	Icon = "",
	PremiumOnly = false
})
local Section = Tab1:AddSection({
	Name = "Reacts R6"
})
Tab1:AddButton({
	Name = "React St ",
	Callback = function(Value)
        if game.Lighting[game.Players.LocalPlayer.Name].PreferredFoot.Value == 1 then
            DistanceReach = tonumber(Value)
            game.Players.LocalPlayer.Character["Right Leg"].Size = Vector3.new(Value, 2, Value)
        elseif game.Lighting[game.Players.LocalPlayer.Name].PreferredFoot.Value == 2 then
            DistanceReach = tonumber(Value)
            game.Players.LocalPlayer.Character["Left Leg"].Size = Vector3.new(Value, 2, Value)
        end
  	end    
})

Tab1:AddButton({
	Name = " React CM ",
	Callback = function(Value)
        if game.Lighting[game.Players.LocalPlayer.Name].PreferredFoot.Value == 1 then
            DistanceReach = tonumber(Value)
            game.Players.LocalPlayer.Character["Right Leg"].Size = Vector3.new(Value, 1, Value)
        elseif game.Lighting[game.Players.LocalPlayer.Name].PreferredFoot.Value == 1 then
            DistanceReach = tonumber(Value)
            game.Players.LocalPlayer.Character["Left Leg"].Size = Vector3.new(Value, 1, Value)
        end
  	end    
})

Tab1:AddButton({
	Name = " React Dribble ",
	Callback = function(Value)
        if game.Lighting[game.Players.LocalPlayer.Name].PreferredFoot.Value == 1 then
            DistanceReach = tonumber(Value)
            game.Players.LocalPlayer.Character["Right Leg"].Size = Vector3.new(Value, 0.2, Value)
        elseif game.Lighting[game.Players.LocalPlayer.Name].PreferredFoot.Value == 1 then
            DistanceReach = tonumber(Value)
            game.Players.LocalPlayer.Character["Left Leg"].Size = Vector3.new(Value, 0.2, Value)
        end
  	end    
})

local Tab2 = Window:MakeTab({
	Name = "Graphics",
	Icon = "",
	PremiumOnly = false
})

local Section = Tab2:AddSection({
	Name = ""
})

Tab2:AddButton({
	Name = "Make The Character Glowy (For Better Looks)",
	Callback = function()

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local function makePartGlow(part)
    local surfaceLight = Instance.new("SurfaceLight")
    surfaceLight.Brightness = 10 
    surfaceLight.Range = 12 
    surfaceLight.Color = Color3.fromRGB(128, 0, 255) 
    surfaceLight.Face = Enum.NormalId.Top 
    surfaceLight.Parent = part
end
for _, part in pairs(character:GetChildren()) do
    if part:IsA("BasePart") then
        makePartGlow(part)
    end
end
player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    for _, part in pairs(character:GetChildren()) do
        if part:IsA("BasePart") then
            makePartGlow(part)
        end
    end
end)


  	end    
})


Tab2:AddButton({
	Name = "RTX Sunny",
	Callback = function()
local Lighting = game:GetService("Lighting")

for _, effect in pairs(Lighting:GetChildren()) do
    if effect:IsA("PostEffect") then
        effect:Destroy()
    end
end

Lighting.Ambient = Color3.fromRGB(255, 255, 255)
Lighting.Brightness = 2
Lighting.OutdoorAmbient = Color3.fromRGB(127, 127, 127)
Lighting.GlobalShadows = true
Lighting.EnvironmentDiffuseScale = 1
Lighting.EnvironmentSpecularScale = 1

local bloom = Instance.new("BloomEffect")
bloom.Intensity = 0.7
bloom.Threshold = 0.1
bloom.Size = 24
bloom.Parent = Lighting

local sunRays = Instance.new("SunRaysEffect")
sunRays.Intensity = 0.1
sunRays.Spread = 1
sunRays.Parent = Lighting

local colorCorrection = Instance.new("ColorCorrectionEffect")
colorCorrection.TintColor = Color3.fromRGB(255, 240, 214)
colorCorrection.Brightness = 0.1
colorCorrection.Contrast = 0.1
colorCorrection.Saturation = 0.2
colorCorrection.Parent = Lighting

local depthOfField = Instance.new("DepthOfFieldEffect")
depthOfField.InFocusRadius = 10
depthOfField.NearIntensity = 0.5
depthOfField.FarIntensity = 0.1
depthOfField.Parent = Lighting

local blur = Instance.new("BlurEffect")
blur.Size = 2
blur.Parent = Lighting

local atmosphere = Instance.new("Atmosphere")
atmosphere.Density = 0.3
atmosphere.Offset = 0.25
atmosphere.Glare = 0.1
atmosphere.Haze = 0.4
atmosphere.Parent = Lighting

  	end    
})

